{
  "hosting": {
    "public": "build",
    "hosting": {
      "redirects": [
        {
          "source": "/firebase/**",
          "destination": "https://wuud.net/",
          "type": 302
        }
      ]
    },
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
